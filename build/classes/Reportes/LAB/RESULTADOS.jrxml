	<subDataset name="RESULTADOS" uuid="cca44c95-b982-4600-97e1-f40805026256">
		<parameter name="ID_COD_DET_RESUL" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT RD.nombre_resultado_exa,RD.valor_de_resultado_analisis,
CASE
when RD.tipo_valor_referencia_resul='N'
then cast( rd.valor_minimo_resul as varchar(10))+'-'+cast(rd.valor_maximo_resul as varchar(10))
when RD.tipo_valor_referencia_resul='S'
then rd.valor_texto_referencia_resul
else '-'
end valor_ref
,UM.descripcion_unidad_medida
FROM LABORATORIO_RESULTADOS_MUESTRA_CABECERA RC,LABORATORIO_RESULTADOS_MUESTRA_DETALLE RD,
LABORATORIO_TOMA_MUESTRA_DETALLE TD,LABORATORIO_ESQUEMA_RESULTADO_ANALISIS ES,LABORATORIO_UNIDAD_MEDIDA_RESULTADO UM
WHERE ES.cod_esquema_resul=RD.cod_esquema_resul AND
TD.cod_det_toma_mu_ana=RC.cod_det_toma_mu_ana AND
RC.cod_cab_resultado_mu_ana=RD.cod_cab_resultado_mu_ana AND
UM.cod_uni_med_exa=RD.cod_uni_med_exa
AND TD.id_cod_det=$P{ID_COD_DET_RESUL}]]>
		</queryString>
		<field name="nombre_resultado_exa" class="java.lang.String"/>
		<field name="valor_de_resultado_analisis" class="java.lang.String"/>
		<field name="valor_ref" class="java.lang.String"/>
		<field name="descripcion_unidad_medida" class="java.lang.String"/>
	</subDataset>
	<parameter name="ID_COD_DET" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select  RC.cod_cab_resultado_mu_ana,ss.SE_DESC+'/'+ce.nombre_clasi_exa AS SERVICIOAREA,ae.nombre_ana_exa,nc.descripcion_nomen_tipo,
CONCAT(SUBSTRING(hc.cod_hc,1,4),'-',SUBSTRING(hc.cod_hc,5,2)) as 'CODIGO' ,
hc.ape_pat+' '+hc.ape_mat+' '+ hc.primer_nombre+' '+hc.segundo_nombre+' '+hc.tercer_nombre as Paciente,
hc.dni, hc.FEC_NAC,case
when cast(datediff(dd,hc.FEC_NAC,GETDATE()) / 30 as int) =0
then convert(varchar(10),datediff(DD,  hc.FEC_NAC, getdate())) + ' D'
when cast( DATEDIFF( DD, hc.FEC_NAC, GETDATE() ) / 365.25 as int)=0
then convert(varchar(10),datediff(month,  hc.FEC_NAC, getdate())) + ' M'
else convert(varchar(10),cast(datediff(dd,CONVERT(date,hc.FEC_NAC,103),GETDATE()) / 365.25 as int))
end AÑOS,hc.sexo,am.NUM_ACTOMEDICO,tc.num_toma_mu_exa,fp.nom_forma_pago,
td.fecha_orden,td.hora_orden,TD.nom_per_solicita,td.hosp_servicio,TD.hab_nom,td.ca_desc,
RC.num_resul_exa,rc.nombre_personal_resultado,rc.nombre_personal_regis_resul,rc.fecha_reg_resul,rc.hora_reg_resul,
dd.id_cod_doc_det
from ADMISION_HISTORIA_CLINICA HC,CAJA_DOCUMENTO_CABECERA DC,CAJA_DOCUMENTO_DETALLE DD,
CAJA_PRECIOS CP, CAJA_NOMENCLATURA_CAJA NC ,CAJA_JERARQUIA_FORMA_PAGO fp,CAJA_ACTO_MEDICO am,
SISTEMA_AREAS SA,SISTEMA_SERVICIO ss,LABORATORIO_ANALISIS_EXAMEN ae,
LABORATORIO_TOMA_MUESTRA_CABECERA tc,LABORATORIO_TOMA_MUESTRA_DETALLE td,
LABORATORIO_RESULTADOS_MUESTRA_CABECERA RC,LABORATORIO_CLASIFICACION_EXAMEN ce
WHERE ce.cod_uni_organica_jerar=sa.AR_ID and
TC.cod_cab_toma_mu_exa=td.cod_cab_toma_mu_exa
and ae.cod_exa_ana=td.cod_exa_ana
and tc.id_documento=dc.id_documento
and td.id_cod_det=dd.id_cod_doc_det
and AM.ID_ACTOMEDICO=DC.Id_ActoMedico
AND SA.AR_ID=NC.AR_ID
and ss.SE_ID=sa.SE_ID
AND NC.cod_nomen_caja=CP.cod_nomen_caja
AND CP.cod_precio=DD.cod_precio
AND fp.cod_jerar_forma_pago=cp.cod_jerar_forma_pago and
 DD.id_documento=DC.id_documento AND DC.id_hc=HC.id_hc
 AND RC.cod_det_toma_mu_ana=TD.cod_det_toma_mu_ana
and tc.estado_toma_mu_exa_cab='A' and td.estado_toma_muestra_det='A'
AND TD.id_cod_det=$P{ID_COD_DET}
group by RC.cod_cab_resultado_mu_ana,ae.cod_exa_ana,hc.id_hc,ss.SE_DESC+'/'+ce.nombre_clasi_exa,
nc.nomen_caja,nc.descripcion_nomen_tipo,
ae.nombre_ana_exa,CONCAT(SUBSTRING(hc.cod_hc,1,4),'-',SUBSTRING(hc.cod_hc,5,2)) ,
hc.ape_pat+' '+hc.ape_mat+' '+ hc.primer_nombre+' '+hc.segundo_nombre+' '+hc.tercer_nombre ,
hc.dni, hc.FEC_NAC,hc.sexo,am.NUM_ACTOMEDICO,tc.num_toma_mu_exa,fp.nom_forma_pago,
td.fecha_orden,td.hora_orden,TD.nom_per_solicita,td.hosp_servicio,TD.hab_nom,td.ca_desc,
RC.num_resul_exa,rc.nombre_personal_resultado,rc.nombre_personal_regis_resul,rc.fecha_reg_resul,rc.hora_reg_resul,
dd.id_cod_doc_det]]>
	</queryString>

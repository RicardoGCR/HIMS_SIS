	<queryString>
		<![CDATA[SELECT count(DC.id_documento)as 'CANTIDAD',DC.cod_usu, SUBSTRING(DC.fecha_actu,1,2)AS DIA,DC.fecha_actu AS DIAMES,
(SELECT TOP 1 upper( UE_DESC ) FROM SISTEMA_UNIDAD_EJECUTORA) AS DESCRIP,
(SELECT TOP 1 UE_LOGO  FROM SISTEMA_UNIDAD_EJECUTORA)AS'IMAGEN',PP.ape_pat_per+' '+PP.ape_mat_per+' '+PP.nombres_per AS PERSONAL,
U.Usu_Usuario
FROM
CAJA_DOCUMENTO_CABECERA DC,
USUARIO U,
PERSONAL_PERSONAL PP
WHERE
PP.cod_per=U.cod_per
AND DC.cod_usu=U.Usu_Codigo
group by DC.cod_usu,DC.fecha_actu,PP.ape_pat_per,PP.ape_mat_per,PP.nombres_per ,
U.Usu_Usuario
ORDER BY DC.fecha_actu]]>
	</queryString>
